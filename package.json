    "@nuxtjs/eslint-config-typescript": "10.0.0",
    "@release-it/conventional-changelog": "4.3.0",
    "@types/jest": "28.1.1",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "8.17.0",
    "husky": "^8.0.1",
    "jest": "28.1.1",
