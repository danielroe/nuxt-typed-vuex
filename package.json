{
  "name": "nuxt-typed-vuex",
  "version": "0.1.10",
  "description": "A typed store accessor for Nuxt.",
  "repository": "danielroe/nuxt-typed-vuex",
  "license": "MIT",
  "contributors": [
    {
      "name": "Daniel Roe <daniel@danielcroe.com>",
      "url": "https://github.com/danielroe"
    }
  ],
  "keywords": [
    "vuex",
    "nuxt",
    "typescript",
    "vue",
    "vuejs",
    "nuxtjs",
    "nuxt-module"
  ],
  "files": [
    "lib",
    "lib/utils.d.ts"
  ],
  "main": "lib/index.js",
  "types": "lib/utils.d.ts",
  "scripts": {
    "lint": "eslint --ext .js,.ts src",
    "build": "tsc",
    "prepare": "yarn build",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "generate": "yarn build && yarn nuxt generate test/fixture",
    "test": "yarn lint && yarn generate && jest && tsc --noImplicitAny --noEmit test/accessor.test.ts && yarn tsd"
  },
  "tsd": {
    "directory": "test/tsd"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@nuxt/typescript-build": "^0.3.4",
    "@nuxt/typescript-runtime": "^0.2.4",
    "@nuxtjs/eslint-config": "^1.1.2",
    "@types/jest": "^24.0.23",
    "@types/normalize-path": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "codecov": "^3.6.1",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "nuxt-edge": "latest",
    "standard-version": "^7.0.0",
    "ts-loader": "^6.2.1",
    "tsd": "^0.11.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "normalize-path": "^3.0.0"
  },
  "peerDependencies": {
    "vuex": "*"
  },
  "optionalDependencies": {
    "@nuxt/types": "*"
  }
}
